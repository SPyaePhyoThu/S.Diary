
{
    "version": 2,
    "builds" :[
        { "src": "public/html/ResetPasswordPage.html", "use": "@vercel/static" },
        { "src": "public/html/img", "use": "@vercel/static" },
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[ {
        "src": "/(.*)",
        "dest": "public/html/ResetPasswordPage.html"
    },
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}